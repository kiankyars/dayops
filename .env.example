# Audio ingestion
VOICE_MEMOS_DIR=/absolute/path/to/voice-memos
PLAN_TEMPLATE_PATH=/absolute/path/to/plan.md

# Obsidian
OBSIDIAN_VAULT_DIR=/absolute/path/to/obsidian-vault
OBSIDIAN_DAILY_NOTES_SUBDIR=notes

# COO state
COO_STATE_DIR=.coo_state
ROLLBACK_SNAPSHOT_DIR=.coo_state/snapshots

# Gemini
GEMINI_API_KEY=
GEMINI_MODELS=gemini-2.5-flash,gemini-2.5-flash-lite,gemini-1.5-pro

# Google Calendar
GOOGLE_CALENDAR_ID=primary
GOOGLE_OAUTH_TOKEN_PATH=/absolute/path/to/google_oauth_token.json
# GOOGLE_SERVICE_ACCOUNT_FILE=/absolute/path/to/service_account.json

# Strava (required)
STRAVA_CLIENT_ID=
STRAVA_CLIENT_SECRET=
STRAVA_REFRESH_TOKEN=

# Runtime
TIMEZONE=America/Los_Angeles
AUTO_APPLY=true
TRASH_PROCESSED_MEMOS=true
HYDRATE_MAX_RETRIES=6
HYDRATE_RETRY_SECONDS=1.5
